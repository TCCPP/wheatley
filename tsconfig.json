    "compilerOptions": {
        "target": "es2021",
        "module": "commonjs",
        "strict": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "allowJs": true,
        "strictPropertyInitialization": false,
        "noImplicitOverride": true
